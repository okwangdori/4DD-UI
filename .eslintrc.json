{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "eslint:recommended",
      "prettier"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}